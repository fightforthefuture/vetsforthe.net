{
  "name": "bfnn-nuxt",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "Ken Mickles <ken@37i.net>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "pregenerate": "npm run generate-signatures && npm run generate-pdfs",
    "generate-signatures": "node scripts/generate-signatures.js",
    "generate-pdfs": "bash scripts/generate-all-pdfs.sh",
    "generate-pdf": "node scripts/generate-pdfs.js",
    "deploy": "aws s3 sync dist s3://www.vetsforthe.net --acl public-read --delete",
    "deploy-branch": "bash scripts/deploy-branch.sh"
  },
  "dependencies": {
    "nuxt": "^1.4.1"
  },
  "devDependencies": {
    "@nuxtjs/google-analytics": "^2.0.2",
    "axios": "^0.18.0",
    "babel-eslint": "^8.2.1",
    "bluebird": "^3.5.1",
    "dotenv": "^5.0.1",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0",
    "google-spreadsheet": "^2.0.5",
    "lodash": "^4.17.5",
    "node-sass": "^4.9.2",
    "normalize.css": "^8.0.0",
    "nuxt-sass-resources-loader": "^2.0.3",
    "pdfkit": "^0.8.3",
    "sass-loader": "^7.0.3"
  }
